            Function WhichType(ByVal varname)
                If varname=Null Then 
                    WhichType="[null]"
                ElseIf IsArray(varname) Then 
                    WhichType="[Array]"
                ElseIf IsObject(varname) Then
                    WhichType="[Object]"
                ElseIf IsNumeric(varname) Then 
                    If InStr(varname,".")Then 
                        WhichType="[Float]"
                    Else
                        WhichType="[Int]"
                    End If        
                End If
            End Function



